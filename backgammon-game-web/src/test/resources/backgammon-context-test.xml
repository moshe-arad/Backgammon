<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

<context:annotation-config />

<bean id="firstPlayer" class="org.moshe.arad.game.player.Player" >
	<qualifier value="firstPlayer" />
	<constructor-arg name="id" value="021777777" />
	<constructor-arg name="firstName" value="Moshe-test" />
	<constructor-arg name="lastName" value="Arad-test" />
	<constructor-arg name="age" value="13" />
</bean>

<bean id="secondPlayer" class="org.moshe.arad.game.player.Player" >
	<qualifier value="secondPlayer" />
	<constructor-arg name="id" value="056666663" />
	<constructor-arg name="firstName" value="James-test" />
	<constructor-arg name="lastName" value="Brown-test" />
	<constructor-arg name="age" value="12" />
</bean>

<bean id="turn" class="org.moshe.arad.game.turn.Turn" factory-method="getInstance" />

<bean class="org.moshe.arad.game.turn.BackgammonTurnOrder" />

</beans>
